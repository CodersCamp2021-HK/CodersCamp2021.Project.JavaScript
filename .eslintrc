{
  "extends": ["eslint:recommended", "airbnb-base", "plugin:prettier/recommended"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/prefer-default-export": 0,
    "import/no-default-export": 2,
    "no-restricted-syntax": "off",
    "no-param-reassign": [2, { "props": false }]
  },
  "overrides": []
}
